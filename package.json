{
  "name": "platzigram-api",
  "version": "1.0.0",
  "description": "Platzigram REST API",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test ava --require babel-register test/**-test.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "author": "Aitor Ibanez <freelance@aitoribanez.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^6.1.2",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "micro": "^4.1.1",
    "request": "^2.74.0",
    "request-promise": "^4.0.2",
    "standard": "^7.1.2",
    "test-listen": "^1.0.0",
    "uuid-base62": "^0.1.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "gravatar": "^1.5.2",
    "http-hash": "^2.0.0",
    "jsonwebtoken": "^7.1.7",
    "token-extractor": "^0.1.6"
  }
}
